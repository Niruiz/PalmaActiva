function Turismo() {
    var automovil = [];
    var cont = "";
    for (var i = 0; i <= 10; i++) {
        var x = {marca: prompt('ingresa la marca de tu coche'),
        modelo: prompt('Ingresa el modelo de tu coche:'),
        año: prompt("ingresa el año"),};
        automovil[i] = x;
    }
    console.log(automovil);
}
Turismo();
